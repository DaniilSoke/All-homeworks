#include <iostream>
int main() {
	int n = 10000;
	std::string t;
	char* s = new char[n];
	std::cin.get(s,n);
	//Полезные элементы
	int k = strlen(s);
	for (int i = 0; i < k / 2; i++) {
		if (s[i] == s[k - 1 - i]) {
			t = "YES";
			continue;
		}
		else {
			t = "NO";
			break;
		}
	}
	std::cout << t;
	return 0;
}
